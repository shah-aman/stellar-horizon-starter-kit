# Database password for PostgreSQL
# Generate a strong password and keep it secure
DB_PASSWORD=your-strong-password-here

# Stellar network to connect to: pubnet, testnet, or futurenet
STELLAR_NETWORK=pubnet

# History archive URLs for the selected network
# Pubnet: https://history.stellar.org/prd/core-live/core_live_001/
# Testnet: https://history.stellar.org/prd/core-testnet/core_testnet_001/
# Futurenet: https://history.stellar.org/prd/core-futurenet/core_futurenet_001/
HISTORY_ARCHIVE_URLS=https://history.stellar.org/prd/core-live/core_live_001/

# Enable/disable ingestion (default: true)
INGEST=true

# Number of ledgers to retain (518400 = ~30 days)
HISTORY_RETENTION_COUNT=518400

# Admin port for metrics and pprof (default: 6060)
ADMIN_PORT=6060

# Log level: debug, info, warn, error (default: info)
LOG_LEVEL=info